{
    "openapi": "3.0.0",
    "info": {
        "title": "ORDS generated API for ivr",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "https://soatst.fdny.org/ords/hris/svy/ivr/"
        }
    ],
    "paths": {
        "/ivr_api": {
            "get": {
                "description": "Retrieve a record from ivr",
                "responses": {
                    "200": {
                        "description": "The queried record.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "ivrApiMethod": {
                                            "type": "string"
                                        },
                                        "ivrApiUrl": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "apiResource",
                        "in": "path",
                        "schema": {
                            "type": "string"
                        },
                        "required": true
                    },
                    {
                        "name": "surveyID",
                        "in": "path",
                        "schema": {
                            "type": "string"
                        },
                        "required": true
                    },
                    {
                        "name": "surveyUserID",
                        "in": "path",
                        "schema": {
                            "type": "string"
                        },
                        "required": true
                    }
                ]
            },
            "post": {
                "description": "Create a new record on ivr",
                "responses": {
                    "201": {
                        "description": "The successfully created record.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "ords_save_dt": {
                                            "$ref": "#/components/schemas/TIMESTAMP"
                                        },
                                        "source": {
                                            "type": "string"
                                        },
                                        "sysdate": {
                                            "$ref": "#/components/schemas/TIMESTAMP"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "parameters": []
            }
        }
    },
    "components": {
        "schemas": {
            "TIMESTAMP": {
                "type": "string",
                "format": "date-time",
                "pattern": "^\\d{4}-[01]\\d-[0123]\\dT[012]\\d:[0-5]\\d:[0-5]\\d(.\\d+)?(Z|([-+][012]\\d:[0-5]\\d))$"
            }
        }
    }
}